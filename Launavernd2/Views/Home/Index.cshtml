@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Launavernd</title>

    
    <link href="~/DX/font-awesome-4.3.0/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/DX/css/bootstrap3.min.css" rel="stylesheet" />
    <link href="~/DX/css/bootstrap3-editable.css" rel="stylesheet" />

    <link href="~/DX/css/angular-material.css" rel="stylesheet" />
    <link href="~/DX/css/google-robodraft-font.css" rel="stylesheet" />

    <link href="~/Content/Site.css" rel="stylesheet" />

    <link href="~/DX/css/dx.spa.css" rel="stylesheet" />
    <link href="~/DX/css/dx.common.css" rel="stylesheet" />
    <link href="~/DX/css/dx.light.css" rel="stylesheet" />
    <style>
        #tabs {
            height: 1080px;
            width: 100%;
        }
    </style>
</head>

<body ng-controller="defaultCtrl">

    <div id="tabPanel" dx-tab-panel="{
                items: tabPanelItems,
                titleTemplate: 'title',
                itemTemplate: 'tab',
                onInitialized: tabInit,
                onSelectionChanged: tabChanged,
                onTitleClick: titleClick,
                height: 1080,
                animationEnabled: true
             }">
        <div data-options="dxTemplate : { name: 'title' } ">
            <table><tr><td><i class="fa fa-{{icon}}" style="font-size: {{iconSize}}; color: {{iconColor}}"></i></td><td style="padding-left:8px;">{{title}}</td></tr></table>
        </div>

        @* ----- TAB0 Forsíða ------*@
        <div data-options="dxTemplate : { name: 'tab0' } ">
            <div style="margin:25px;">
                @Html.Partial("_tab0_Forsida")
            </div>
        </div>

        @* ----- TAB1 Persónuupplýsingar ------*@
        <div data-options="dxTemplate : { name: 'tab1' } ">
            <div style="margin:25px;">
                @Html.Partial("_tab1_Personuupplysingar")
            </div>
        </div>

        @* ----- TAB3 Fjármál ------*@
        <div data-options="dxTemplate : { name: 'tab2' } ">
            <div style="margin:25px;">
                @Html.Partial("_tab2_Fjarmal")
            </div>
        </div>

        @* ----- TAB4 Útreikningur ------*@
        <div data-options="dxTemplate : { name: 'tab3' } ">
            <div style="margin:25px;">
                @Html.Partial("_tab3_Utreikningur")
            </div>
        </div>

    </div>
       
    <script src="~/DX/js/jquery-2.1.3.min.js"></script>
    <script src="~/DX/js/bootstrap3.min.js"></script>
    <script src="~/DX/js/bootstrap3-editable.min.js"></script>

    <script src="~/DX/js/globalize.min.js"></script>
    <script src="~/DX/globalize-de.js"></script>

    <script src="~/DX/js/angular-1.3.11.min.js"></script>
    <script src="~/DX/js/angular-1.3.11-sanitize.min.js"></script>
    <script src="~/DX/js/angular-1.3.11-animate.min.js"></script>
    <script src="~/DX/js/angular-1.3.11-aria.min.js"></script>
    <script src="~/DX/js/angular-material.min.js"></script>
    <script src="~/DX/js/angular-route.min.js"></script>

    <script src="~/DX/js/dx.webappjs.js"></script>

    <script src="~/Scripts/_utils.js"></script>
    <script src="~/Scripts/_format.js"></script>
    <script src="~/Scripts/_calc.js"></script>
    <script src="~/Scripts/_persona.js"></script>
    <script src="~/Scripts/_data.js"></script>

    <script src="~/Scripts/_main.js"></script>
</body>
</html>
